{
  "name": "simple-dungeon-crawler",
  "version": "12.0.0",
  "description": "A simple dungeon crawling game with console graphics",
  "private": true,
  "scripts": {
    "eslint": "eslint src/*.ts",
    "check_build": "webpack --config webpack.dev.js",
    "build": "webpack --config webpack.dev.js && cp -r bin/ dev/bin/",
    "dist": "webpack --config webpack.prod.js && cp -r bin/ dev/bin/",
    "start": "webpack-dev-server --config webpack.dev.js --open",
    "watch": "webpack --config webpack.dev.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JackStouffer/Simple-Dungeon-Crawler.git"
  },
  "author": "Jack Stouffer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JackStouffer/Simple-Dungeon-Crawler/issues"
  },
  "homepage": "https://github.com/JackStouffer/Simple-Dungeon-Crawler#readme",
  "devDependencies": {
    "@types/howler": "^2.2.1",
    "@types/html-webpack-plugin": "^3.2.4",
    "@types/lodash": "^4.14.167",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "eslint": "^7.17.0",
    "expose-loader": "^1.0.1",
    "html-webpack-plugin": "^4.5.1",
    "proxyquire": "^2.1.3",
    "source-map-support": "^0.5.19",
    "ts-loader": "^8.0.14",
    "typescript": "^4.0.5",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.1",
    "webpack-merge": "^5.3.0"
  },
  "dependencies": {
    "ape-ecs": "^1.3.1",
    "howler": "^2.2.1",
    "lodash": "^4.17.20",
    "pixi-filters": "^3.2.2",
    "pixi-particles": "^4.3.0",
    "pixi.js": "^5.3.7",
    "tiled-types": "^1.2.1"
  }
}
